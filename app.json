{
  "configVersion": "v3",
  "app": {
    "appId": 1023438,
    "appName": "Tasks NC",
    "appType": "app",
    "version": {
      "code": 100,
      "name": "1.0"
    },
    "icon": "icon.png",
    "vender": "ether-strannik",
    "description": "Nextcloud Tasks for ZeppOS"
  },
  "permissions": [
    "device:os.notification",
    "data:os.device.info",
    "device:os.alarm",
    "device:os.local_storage",
    "device:os.geolocation"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.2",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/amazfit/HomeScreen",
            "page/amazfit/TaskEditScreen",
            "page/amazfit/SettingsScreen",
            "page/amazfit/AboutScreen",
            "page/amazfit/TaskListPickerScreen",
            "page/amazfit/ReminderPickerScreen",
            "page/amazfit/CategoryPickerScreen",
            "page/amazfit/AddEventScreen",
            "page/amazfit/FontSizeSetupScreen",
            "page/amazfit/DonatePage",
            "page/amazfit/MarkdownReader",
            "page/amazfit/NewNoteScreen"
          ]
        },
        "app-service": {
          "services": ["app-service/index"]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r"
        }
      ],
      "designWidth": 480
    }
  },
  "debug": false,
  "defaultLanguage": "en-US"
}
